<?xml version="1.0" encoding="utf-8"?>
<!--输入框样式设置-->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <corners android:radius="20dp"/>
    <solid android:color="@color/white"/>
    <!-- 设置边框宽度和颜色 -->
    <stroke
        android:width="3dip"
        android:color="#94ac7b" />
    <!-- 设置边距 -->
    <padding
        android:bottom="5dp"
        android:left="10dp"
        android:right="5dp"
        android:top="5dp" />
</shape>